{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Checking ESP32",
  "version": "0.1.0",
  "identifier": "wzx",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Checking ESP32",
        "width": 1200,
        "height": 742,
        "minWidth": 1200,
        "minHeight": 742
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "resources": [
      "./config/*"
    ],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}